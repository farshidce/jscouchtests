#couch.app: couch.app.tpl
#	modules=`find . -name "couch*.erl" -exec basename {} .erl \; | tr '\n' ',' | sed "s/,$$//"`; \
#	sed -e "s|%package_name%|Apache CouchDB|g" \
#			-e "s|%version%|1.2.0a-fec96b1-git|g" \
